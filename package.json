  },
  "devDependencies": {
    "@fastify/formbody": "7.4.0",
    "@podium/eslint-config": "1.0.0",
    "@podium/podlet": "5.1.17",
    "@podium/test-utils": "2.5.2",
    "@semantic-release/changelog": "6.0.3",
    "@semantic-release/git": "10.0.1",
    "eslint": "9.9.1",
    "fastify": "4.28.1",
    "npm-run-all2": "6.2.3",
    "prettier": "3.3.3",
    "semantic-release": "23.1.1",
    "tap": "18.8.0",
    "typescript": "5.6.2"
