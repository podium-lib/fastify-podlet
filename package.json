  },
  "devDependencies": {
    "@fastify/formbody": "8.0.2",
    "@podium/eslint-config": "1.0.5",
    "@podium/podlet": "5.2.4",
    "@podium/semantic-release-config": "2.0.0",
    "@podium/test-utils": "2.5.2",
    "@podium/typescript-config": "1.0.0",
    "eslint": "9.17.0",
    "fastify": "5.2.2",
    "npm-run-all2": "6.2.6",
    "prettier": "3.4.2",
    "semantic-release": "24.2.0",
    "tap": "21.0.1",
    "typescript": "5.7.2"
  }
