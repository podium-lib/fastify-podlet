{
  "name": "@podium/fastify-podlet",
  "version": "3.0.11",
  "type": "module",
  "description": "Fastify plugin for Podium Podlet",
  "main": "lib/podlet-plugin.js",
  "types": "types/podlet-plugin.d.ts",
  "repository": {
    "type": "git",
