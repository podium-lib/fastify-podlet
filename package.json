    "@podium/eslint-config": "1.0.19",
    "@podium/podlet": "5.3.2",
    "@podium/semantic-release-config": "3.0.0",
    "@podium/test-utils": "3.0.20",
    "@podium/typescript-config": "1.0.0",
    "eslint": "9.39.1",
    "fastify": "5.6.2",
    "npm-run-all2": "8.0.4",
    "prettier": "3.7.4",
